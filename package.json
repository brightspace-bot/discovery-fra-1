{
  "name": "discovery",
  "appId": "urn:d2l:fra:class:discovery",
  "description": "Discovery",
  "version": "2.1.19",
  "license": "BSD-3-Clause",
  "repository": "git@github.com:Brightspace/discovery-fra.git",
  "scripts": {
    "lint": "npm run lint:javascript",
    "lint:javascript": "eslint src --ext js --ignore-path .gitignore",
    "test": "wct test --skip-plugin sauce --npm",
    "test:integration": "polymer build # test that psk builds without error with the CLI",
    "test:polymer:sauce": "wct test --skip-plugin local --npm",
    "build": "gulp build && polymer build && mv ./build/es5-bundled/* ./build && rmdir \"./build/es5-bundled\"",
    "build:debug": "gulp build && polymer build --name debug && mv ./build/debug/* ./build && rmdir ./build/debug",
    "start": "npm run build && npm run frau:build-config && npm run frau:resolve",
    "start:debug": "npm run build:debug && npm run frau:build-config && npm run frau:resolve",
    "ifrautoaster": "ifrautoaster --config ./ifrautoaster-config.json",
    "frau:build-config": "frau-appconfig-builder",
    "frau:resolve": "frau-local-appresolver",
    "frau:publish": "frau-publisher",
    "frau:release": "npm run frau:publish",
    "install-attest": "npm install --no-save attest"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1.40.3",
    "@polymer/app-route": "^3.0.0-pre.15",
    "@polymer/iron-pages": "^3.0.0-pre.15",
    "@polymer/iron-resizable-behavior": "^3.0.0",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/polymer": "3.1.0",
    "d2l-activities": "BrightspaceHypermediaComponents/activities#semver:^3",
    "d2l-alert": "BrightspaceUI/alert#semver:^4",
    "d2l-breadcrumbs": "BrightspaceUI/breadcrumbs#semver:^2",
    "d2l-button": "BrightspaceUI/button#semver:^5",
    "d2l-colors": "BrightspaceUI/colors#semver:^4",
    "d2l-dropdown": "BrightspaceUI/dropdown#semver:^7",
    "d2l-facet-filter-sort": "BrightspaceUI/facet-filter-sort#semver:^3",
    "d2l-fetch": "Brightspace/d2l-fetch#semver:^2",
    "d2l-fetch-auth": "github:Brightspace/d2l-fetch-auth",
    "d2l-fetch-dedupe": "^1.1.0",
    "d2l-hypermedia-constants": "Brightspace/d2l-hypermedia-constants#semver:^6",
    "d2l-inputs": "BrightspaceUI/inputs#semver:^2",
    "d2l-link": "BrightspaceUI/link#semver:^5",
    "d2l-loading-spinner": "BrightspaceUI/loading-spinner#semver:^7",
    "d2l-localize-behavior": "BrightspaceUI/localize-behavior#semver:^2",
    "d2l-menu": "BrightspaceUI/menu#semver:^2",
    "d2l-offscreen": "BrightspaceUI/offscreen#semver:^4",
    "d2l-organization-hm-behavior": "Brightspace/organization-hm-behavior#semver:^3",
    "d2l-organizations": "github:BrightspaceHypermediaComponents/organizations#semver:^5",
    "d2l-typography": "BrightspaceUI/typography#semver:^7",
    "dompurify": "^1.0.10",
    "fastdom": "^1.0.8",
    "promise-polyfill": "8.1.0",
    "siren-parser": "^8.2.0",
    "siren-sdk": "github:BrightspaceHypermediaComponents/siren-sdk#semver:^1",
    "url-polyfill": "^1.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@d2l/ifrautoaster": "^1.1.7",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-test-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "babel-eslint": "^10.0.1",
    "bower": "^1.8.4",
    "del": "^3.0.0",
    "depd": "^2.0.0",
    "eslint": "^5.1.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.5",
    "frau-appconfig-builder": "^1.0.0",
    "frau-local-appresolver": "^1.0.0",
    "frau-publisher": "^2.7.1",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-ejs": "^3.1.3",
    "gulp-rename": "^1.2.3",
    "merge-stream": "^1.0.1",
    "require-dir": "^1.2.0",
    "wct-browser-legacy": "^1.0.2",
    "wct-none": "^0.1.1",
    "web-component-tester": "^6.9.2"
  },
  "engines": {
    "node": ">=8.0"
  },
  "config": {
    "frauAppConfigBuilder": {
      "appFile": "index.html",
      "dist": "./build",
      "envVar": "CI",
      "loader": "iframe",
      "port": 3000
    },
    "frauLocalAppResolver": {
      "appClass": "urn:d2l:fra:class:discovery",
      "dist": "./build",
      "port": 3000
    },
    "frauPublisher": {
      "files": "./build/**",
      "moduleType": "app",
      "targetDirectory": "discovery",
      "creds": {
        "key": "AKIAIX4OHXOJHJAHALOQ",
        "secretVar": "FRAU_PUBLISHER_SECRET"
      },
      "devTagVar": "DEV_VER",
      "versionVar": "PACKAGE_VER"
    }
  }
}
